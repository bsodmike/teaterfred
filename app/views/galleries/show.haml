%p#notice= notice
%p
  %table
    - @gallery.images.each do |gallery|
      %tr
        %td= gallery.title
        %td.image_box
          = link_to ( image_tag gallery.image, :width => 100, :height => 100), '#'

  - if current_user
    = link_to 'Rediger', edit_gallery_path(@gallery)
    |
    = link_to 'Tilbage', galleries_path